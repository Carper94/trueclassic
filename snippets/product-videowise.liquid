<div
  class='reeview-app-widget'
  id='reeview-app-widget_63dd492c555d61002a0f56d6'
></div>
<br>
{% if product.metafields.custom.icon_links.value[0] != blank %}
  <div class='product__icons'>
    <ul class='product__icons--list'>
      {% if product.metafields.custom.icon_links.value[0] != blank
        and product.metafields.custom.icon_links.value[1] != blank
      %}
        <li class='product__icon--item'>
          <img
            src='{{ product.metafields.custom.icon_links.value[0] }}'
            alt='icon'
            width='50'
            height='50'
            loading='lazy'
          >
          <p>{{ product.metafields.custom.icon_links.value[1] }}</p>
        </li>
      {% endif %}

      {% if product.metafields.custom.icon_links.value[2] != blank
        and product.metafields.custom.icon_links.value[3] != blank
      %}
        <li class='product__icon--item'>
          <img
            src='{{ product.metafields.custom.icon_links.value[2] }}'
            alt='icon'
            width='50'
            height='50'
            loading='lazy'
          >
          <p>{{ product.metafields.custom.icon_links.value[3] }}</p>
        </li>
      {% endif %}

      {% if product.metafields.custom.icon_links.value[4] != blank
        and product.metafields.custom.icon_links.value[5] != blank
      %}
        <li class='product__icon--item'>
          <img
            src='{{ product.metafields.custom.icon_links.value[4] }}'
            alt='icon'
            width='50'
            height='50'
            loading='lazy'
          >
          <p>{{ product.metafields.custom.icon_links.value[5] }}</p>
        </li>
      {% endif %}

      {% if product.metafields.custom.icon_links.value[6] != blank
        and product.metafields.custom.icon_links.value[7] != blank
      %}
        <li class='product__icon--item'>
          <img
            src='{{ product.metafields.custom.icon_links.value[6] }}'
            alt='icon'
            width='50'
            height='50'
            loading='lazy'
          >
          <p>{{ product.metafields.custom.icon_links.value[7] }}</p>
        </li>
      {% endif %}

      {% if product.metafields.custom.icon_links.value[8] != blank
        and product.metafields.custom.icon_links.value[9] != blank
      %}
        <li class='product__icon--item'>
          <img
            src='{{ product.metafields.custom.icon_links.value[8] }}'
            alt='icon'
            width='50'
            height='50'
            loading='lazy'
          >
          <p>{{ product.metafields.custom.icon_links.value[9] }}</p>
        </li>
      {% endif %}

      {% if product.metafields.custom.icon_links.value[10] != blank
        and product.metafields.custom.icon_links.value[11] != blank
      %}
        <li class='product__icon--item'>
          <img
            src='{{ product.metafields.custom.icon_links.value[10] }}'
            alt='icon'
            width='50'
            height='50'
            loading='lazy'
          >
          <p>{{ product.metafields.custom.icon_links.value[11] }}</p>
        </li>
      {% endif %}

      {% if product.metafields.custom.icon_links.value[12] != blank
        and product.metafields.custom.icon_links.value[13] != blank
      %}
        <li class='product__icon--item'>
          <img
            src='{{ product.metafields.custom.icon_links.value[12] }}'
            alt='icon'
            width='50'
            height='50'
            loading='lazy'
          >
          <p>{{ product.metafields.custom.icon_links.value[13] }}</p>
        </li>
      {% endif %}
    </ul>
  </div>
{% endif %}
{% comment %}
   {% if section.settings.show_icons %}
    <br>
    <div
      class='product-icons'
    >
      {% for i in (1..6) %}
        {% assign icon_image = 'icon_image_' | append: i %}
        {% assign icon_text = 'icon_text_' | append: i %}
        <div class='product-icons__icon'>
          <div class='product-icons__svg'>{{ section.settings[icon_image] }}</div>
          <p>{{ section.settings[icon_text] }}</p>
        </div>
      {% endfor %}
    </div>
  {% endif %}
{% endcomment %}
