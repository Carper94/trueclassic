{
  "name": "true-classic",
  "version": "3.2.0",
  "description": "True Classic Theme",
  "scripts": {
    "start": "npm run login && npm run dev",
    "login": "shopify theme dev --store trueclassictees-com",
    "login:sandbox": "shopify login --store=wlcr-sandbox.myshopify.com/",
    "dev": "shopify theme dev --store trueclassictees-com & npm run webpack-watch",
    "dev:sync": "shopify theme dev --store trueclassictees-com --theme-editor-sync & npm run webpack-watch",
    "devnet": "shopify theme dev --host \"$(ipconfig getifaddr en0)\" --store trueclassictees-com & npm run webpack-watch",
    "devnet:sync": "shopify theme dev --host \"$(ipconfig getifaddr en0)\" --store trueclassictees-com --theme-editor-sync & npm run webpack-watch",
    "webpack-watch": "NODE_ENV=development webpack --watch --config build/webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "npx eslint -c ./.eslintrc.js ./source",
    "eslint-fix": "prettier --write $PWD/'source/**/*.{js,jsx}' ",
    "stylelint-fix": "prettier --write $PWD/'source/**/*.{css,scss}' ",
    "clear-assets": "node ./build/tools/clearBuiltAssets.js",
    "scaffold": "node ./build/tools/scaffoldWizard.js",
    "pull": "shopify theme pull --development --nodelete",
    "pull-file": "shopify theme pull --development --only $@"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trueclassic/true-classic.git"
  },
  "author": "True Classic Development Team",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/trueclassic/true-classic/issues"
  },
  "homepage": "https://github.com/trueclassic/true-classic#readme",
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "jest": {
    "moduleNameMapper": {
      "^LAYOUT(.*)$": "<rootDir>/source/scripts/layout$1",
      "^SECTIONS(.*)$": "<rootDir>/source/scripts/sections$1",
      "^TEMPLATES(.*)$": "<rootDir>/source/scripts/templates$1",
      "^UTILS(.*)$": "<rootDir>/source/scripts/utilities$1",
      "^COMPONENTS(.*)$": "<rootDir>/source/scripts/components$1",
      "^ACCESSIBILITY(.*)$": "<rootDir>/source/scripts/utilities/accessibility$1",
      "^CUSTOMERS(.*)$": "<rootDir>/source/scripts/utilities/customers$1",
      "^DOM(.*)$": "<rootDir>/source/scripts/utilities/DOM$1",
      "^WINDOW(.*)$": "<rootDir>/source/scripts/utilities/window$1",
      "^FETCH(.*)$": "<rootDir>/source/scripts/utilities/fetch$1"
    }
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-react-jsx": "^7.17.12",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@shopify/theme-a11y": "^4.1.0",
    "@shopify/theme-cart": "^4.1.0",
    "@shopify/theme-currency": "^4.1.0",
    "@shopify/theme-product": "^4.1.0",
    "@shopify/theme-product-form": "^4.1.0",
    "@shopify/theme-sections": "^4.1.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.2.2",
    "chalk": "^4.1.0",
    "config-yaml": "^1.1.6",
    "copy-webpack-plugin": "^9.0.0",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^3.0.0",
    "dotenv": "^16.0.1",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-preact": "^1.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-compat": "^4.1.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-shopify": "^7.0.0",
    "eslint-webpack-plugin": "^2.5.4",
    "focus-trap": "^6.3.0",
    "graphql-request": "^4.3.0",
    "lazysizes": "^5.3.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^1.3.5",
    "modern-css-reset": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "pickoneitem": "^1.0.1",
    "postcss": "^8.2.4",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^5.3.0",
    "postcss-preset-env": "^7.8.2",
    "postcss-pxtorem": "^6.0.0",
    "preact": "^10.13.2",
    "prettier": "^2.8.8",
    "prompts": "^2.4.1",
    "qs": "^6.11.0",
    "rimraf": "^3.0.2",
    "stylelint": "^15.4.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended": "^11.0.0",
    "stylelint-config-standard": "^32.0.0",
    "stylelint-prettier": "^3.0.0",
    "svgo": "^2.3.0",
    "swiper": "^8.1.5",
    "uuid": "^8.3.2",
    "webpack": "^5.37.0",
    "webpack-merge": "^5.7.3",
    "webpack-remove-empty-scripts": "^0.7.1",
    "what-input": "^5.2.10",
    "xml2json": "^0.12.0"
  },
  "devDependencies": {
    "jest": "^29.2.1",
    "webpack-cli": "^5.0.1"
  }
}