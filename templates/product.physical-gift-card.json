        "be0eaf5d-a508-4c56-8e8c-e4018c60c4e4": {
          "type": "card",
          "settings": {
            "title": "SAVES TIME",
            "text": "No need to wrack your brain for the perfect size, color, or style. Let him pick his favorites from the brand guys trust most."
          }
        },
        "a86c2a71-a547-4a4f-8381-a8323a41e54d": {
          "type": "card",
          "settings": {
            "title": "NEVER EXPIRES",
            "text": "Look good without pressure. There’s no time limit or expiration date with our gift cards. He can shop around on his clock."
          }
        }
      },
